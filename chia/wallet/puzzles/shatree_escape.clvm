(mod (to_be_hashed)
  ; this program takes a hash tree but escapes a node if it is a list with 1997 as the first value

  ; takes a lisp tree and returns the hash of it
  (defun sha256tree1 (TREE)
      (if (l TREE)
        (if (= (f TREE) 1997)
          (f (r TREE))
          (sha256 2 (sha256tree1 (f TREE)) (sha256tree1 (r TREE)))
        )
        (sha256 1 TREE)
      )
  )

  (sha256tree1 to_be_hashed)
)
